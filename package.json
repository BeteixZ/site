{
  "name": "bindows-site",
  "type": "module",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "eleventy --serve",
    "build:distill": "cd src/assets/distill && npm run build",
    "copy:distill": "shx rm -rf src/assets/js/* && shx rm -rf src/assets/css/* && shx rm -rf src/assets/images/distill/* && shx mkdir -p src/assets/js src/assets/css src/assets/images/distill && shx cp src/assets/distill/public/*.js src/assets/js/ && shx cp src/assets/distill/public/*.css src/assets/css/ && shx cp src/assets/distill/public/*.png src/assets/images/distill/ && shx cp -r src/assets/distill/public/graph-level src/assets/images/distill/ && shx cp -r src/assets/distill/public/x-to-graph src/assets/images/distill/",
    "build": "npm-run-all build:distill copy:distill eleventy",
    "eleventy": "eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
