    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "clang-format": "~1.2.2",
    "cross-env": "~5.1.6",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.0.3"
  },
